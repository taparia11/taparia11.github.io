            "image_72": "https:\/\/secure.gravatar.com\/avatar\/43320442fdbbaff5c73b457e2b5c74ad.jpg?s=72&d=https%3A%2F%2Fa.slack-edge.com%2Fdf10d%2Fimg%2Favatars%2Fava_0016-72.png",
