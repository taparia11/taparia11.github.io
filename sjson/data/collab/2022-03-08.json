                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "What's is the difference between copywriting and content writing?"
                            }
                        ]
                    }
                ]
            }
        ],
        "thread_ts": "1646732505.484759",
