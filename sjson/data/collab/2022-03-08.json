        "client_msg_id": "0baaeb73-324e-40a3-b486-43e43150cec0",
        "type": "message",
        "text": "What's is the difference between copywriting and content writing?",
        "user": "U034YM1F5DX",
        "ts": "1646733833.408859",
        "team": "TJE58GTJL",
        "user_team": "TJE58GTJL",
        "source_team": "TJE58GTJL",
