    {
        "client_msg_id": "b132a9a0-6c95-4901-9568-36131463b011",
        "type": "message",
        "text": "Would love to get your perspective on incorporating sources into content marketing pieces. This is something I've been trying to do more &amp; <@UJ2MQ8SRH> has written about lately",
        "user": "UJG24P8P6",
        "ts": "1607621829.157300",
        "team": "TJE58GTJL",
        "user_team": "TJE58GTJL",
        "source_team": "TJE58GTJL",
        "user_profile": {
            "avatar_hash": "2de5f7f10d61",
            "image_72": "https:\/\/avatars.slack-edge.com\/2020-06-17\/1188814622853_2de5f7f10d615692b763_72.jpg",
            "first_name": "Josh",
            "real_name": "Josh Spilker",
            "display_name": "Josh Spilker",
            "team": "TJE58GTJL",
            "name": "joshspilker",
