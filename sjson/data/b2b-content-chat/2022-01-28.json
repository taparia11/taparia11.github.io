    {
        "client_msg_id": "3e12ced6-1c8a-4c1c-9707-24f28d99345a",
        "type": "message",
        "text": "If you’re gating it, I’d focus on TOFU and I’d also make sure the content is something the audience can see themselves getting almost immediate ROI from. Templates are often popular.",
        "user": "ULKUFTFPW",
        "ts": "1643391756.661949",
        "team": "TJE58GTJL",
        "user_team": "TJE58GTJL",
        "source_team": "TJE58GTJL",
